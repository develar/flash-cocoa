<?xml version="1.0" ?>
<EventMap xmlns="http://mate.asfusion.com" xmlns:fx="http://ns.adobe.com/mxml/2009">
	<fx:Script><![CDATA[
		import cocoa.modules.events.LoadModuleEvent;
		]]></fx:Script>

	<fx:Declarations>
		<EventHandlers type="{LoadModuleEvent.LOAD_MODULE}">
			<MethodInvoker role="{ModuleManager}" method="load" arguments="{[event.module]}"/>
		</EventHandlers>
	</fx:Declarations>
</EventMap>